{
  "expectation_type" : "expect_column_values_to_not_match_like_pattern",
  "datasets" : [{
    "data" : {
      "a" : ["aaa", "abb", "acc", "add", "bee"],
      "b" : ["aaa", "abb", "acc", "bdd", null],
      "c" : [ null,  null,  null,  null, null]
    },
    "schemas": {
      "spark": {
        "a": "StringType",
        "b": "StringType",
        "c": "StringType"
      },
      "mssql": {
        "a": "VARCHAR",
        "b": "NVARCHAR",
        "c": "NVARCHAR"
      }
    },
    "tests": [
    {
      "title": "negative_test_insufficient_mostly_and_one_non_matching_value",
      "exact_match_out" : false,
      "in": {
        "column": "a",
        "like_pattern": "[a]%",
        "mostly": 0.3
      },
      "out": {
        "success":false,
        "unexpected_index_list": [{"pk_index": 0, "a": "aaa"}, {"pk_index": 1, "a": "abb"}, {"pk_index": 2, "a": "acc"}, {"pk_index": 3, "a": "add"}],
        "unexpected_list": ["aaa", "abb", "acc", "add"]
      },
      "only_for": ["mssql"]
    },
    {
      "title": "positive_test_exact_mostly_w_one_non_matching_value",
      "exact_match_out" : false,
      "in": {
        "column": "a",
        "like_pattern": "[a]%",
        "mostly": 0.2
      },
      "out": {
        "success":true,
        "unexpected_index_list": [{"pk_index": 0, "a": "aaa"}, {"pk_index": 1, "a": "abb"}, {"pk_index": 2, "a": "acc"}, {"pk_index": 3, "a": "add"}],
        "unexpected_list": ["aaa", "abb", "acc", "add"]
      },
      "only_for": ["mssql"]
    },
    {
      "title": "positive_test_sufficient_mostly_w_one_non_matching_value",
      "exact_match_out" : false,
      "in": {
        "column": "a",
        "like_pattern": "[a]%",
        "mostly": 0.1
      },
      "out": {
        "success":true,
        "unexpected_index_list": [{"pk_index": 0, "a": "aaa"}, {"pk_index": 1, "a": "abb"}, {"pk_index": 2, "a": "acc"}, {"pk_index": 3, "a": "add"}],
        "unexpected_list": ["aaa", "abb", "acc", "add"]
      },
      "only_for": ["mssql"]
    },
    {
      "title": "negative_test_one_missing_value_and_insufficent_mostly",
      "exact_match_out" : false,
      "in": {
        "column": "b",
        "like_pattern": "[a]%",
        "mostly": 0.5
      },
      "out": {
        "success":false,
        "unexpected_index_list": [{"pk_index": 0, "b": "aaa"}, {"pk_index": 1, "b": "abb"}, {"pk_index": 2, "b": "acc"}],
        "unexpected_list": ["aaa", "abb", "acc"]
      },
      "only_for": ["mssql"]
    },
    {
      "title": "positive_test_one_missing_value_no_exceptions",
      "exact_match_out" : false,
      "in": {
        "column": "b",
        "like_pattern": "[a]"
      },
      "out": {
        "success":true,
        "unexpected_index_list": [],
        "unexpected_list": []
      },
      "only_for": ["mssql"]
    },
    {
      "title": "positive_test_all_missing_values",
      "exact_match_out" : false,
      "in": {
        "column": "c",
        "like_pattern": "[a]%"
      },
      "out": {
        "success": true,
        "unexpected_index_list": [],
        "unexpected_list": []
      },
      "only_for": ["mssql"]
