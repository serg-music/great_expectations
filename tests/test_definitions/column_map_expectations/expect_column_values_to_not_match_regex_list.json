{
  "expectation_type" : "expect_column_values_to_not_match_regex_list",
  "datasets" : [{
    "data" : {
      "w" : ["111", "222", "333", "123", "321", "444", "456", "654", "555", null],
      "x" : ["man", "plan", "canal", "panama", "hat", "bat", "bit", "bot", "but", "bet"],
      "column_name with space": ["man", "plan", "canal", "panama", "hat", "bat", "bit", "bot", "but", "bet"]
    },
    "schemas": {
      "spark": {
        "w": "StringType",
        "x": "StringType",
        "column_name with space": "StringType"
      }
    },
    "suppress_test_for": ["snowflake"],
    "tests" : [
    {
      "title" : "basic_positive_test",
      "include_in_gallery": true,
      "exact_match_out" : false,
      "in": {
        "column": "w",
        "regex_list": ["\\s+"]
      },
      "out": {
        "unexpected_list": [],
        "unexpected_index_list": [],
        "success": true
      },
      "suppress_test_for": ["sqlite", "mssql"]
    },
    {
      "title" : "positive_test_with_multiple_regexes",
      "exact_match_out" : false,
      "in": {
        "column": "w",
        "regex_list": ["\\s+", "[a-zA-Z]"]
      },
      "out": {
        "unexpected_list": [],
        "unexpected_index_list": [],
        "success": true
      },
      "suppress_test_for": ["sqlite", "mssql"]
    },
    {
      "title" : "basic_negative_test",
      "include_in_gallery": true,
      "exact_match_out" : false,
      "in": {
        "column": "w",
        "regex_list": ["[12]+", "[45]+"]
      },
      "out": {
        "unexpected_list": ["111", "222", "123", "321", "444", "456", "654", "555"],
        "unexpected_index_list": [{"w": "111", "pk_index": 0}, {"w": "222", "pk_index": 1}, {"w": "123", "pk_index": 3}, {"w": "321", "pk_index": 4}, {"w": "444", "pk_index": 5}, {"w": "456", "pk_index": 6}, {"w": "654", "pk_index": 7}, {"w": "555", "pk_index": 8}],
        "success": false
      },
      "suppress_test_for": ["sqlite", "mssql"]
    },
    {
      "title" : "negative_test_with_more_string-ish_strings",
      "exact_match_out" : false,
      "in": {
        "column": "x",
        "regex_list": ["opatomus", "ovat", "h.*t"]
      },
      "out": {
        "unexpected_list": ["hat"],
        "unexpected_index_list": [{"x": "hat", "pk_index": 4}],
        "success": false
      },
      "suppress_test_for": ["sqlite", "mssql"]
    }]
  }]
}
