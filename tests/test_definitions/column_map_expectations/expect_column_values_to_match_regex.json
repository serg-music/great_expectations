{
  "expectation_type" : "expect_column_values_to_match_regex",
  "datasets" : [{
    "data" : {
      "a" : ["aaa", "abb", "acc", "add", "bee"],
      "b" : ["aaa", "abb", "acc", "bdd", null],
      "c" : [ null,  null,  null,  null, null],
      "column_name with space": ["aaa", "abb", "acc", "add", "bee"]
    },
    "schemas": {
      "spark": {
        "a": "StringType",
        "b": "StringType",
        "c": "StringType",
        "column_name with space": "StringType"
      }
    },
    "suppress_test_for": ["snowflake"],
    "tests": [
    {
      "title": "negative_test_insufficient_mostly_and_one_non_matching_value",
      "include_in_gallery": true,
      "exact_match_out" : false,
      "in": {
        "column": "a",
        "regex": "^a",
        "mostly": 0.9
      },
      "out": {
        "success":false,
        "unexpected_index_list": [{"a": "bee", "pk_index": 4}],
        "unexpected_list": ["bee"]
      },
      "suppress_test_for": ["sqlite", "mssql"]
    },
      {
      "title": "negative_test_insufficient_mostly_and_one_non_matching_value_summary_output",
      "include_in_gallery": true,
      "exact_match_out" : false,
      "result_format": "SUMMARY",
      "in": {
        "column": "a",
        "regex": "^a",
        "mostly": 0.9
      },
      "out": {
        "success":false,
        "unexpected_index_list":[{"a": "bee", "pk_index": 4}],
        "unexpected_list": ["bee"]
      },
      "only_for": ["pandas"]
    },
    {
      "title": "positive_test_exact_mostly_w_one_non_matching_value",
      "include_in_gallery": true,
      "exact_match_out" : false,
      "in": {
        "column": "a",
        "regex": "^a",
        "mostly": 0.8
      },
      "out": {
        "success":true,
        "unexpected_index_list": [{"a": "bee", "pk_index": 4}],
        "unexpected_list": ["bee"]
      },
      "suppress_test_for": ["sqlite", "mssql"]
    },
    {
      "title": "positive_test_column_name_has_space",
      "exact_match_out" : false,
      "in": {
        "column": "column_name with space",
        "regex": "^a",
        "mostly": 0.8
      },
      "out": {
        "success":true,
        "unexpected_index_list": [{"column_name with space": "bee", "pk_index": 4}],
        "unexpected_list": ["bee"]
      },
      "suppress_test_for": ["sqlite", "mssql", "bigquery"]
    },
    {
      "title": "positive_test_sufficient_mostly_w_one_non_matching_value",
      "exact_match_out" : false,
      "in": {
        "column": "a",
        "regex": "^a",
        "mostly": 0.7
      },
      "out": {
        "success":true,
        "unexpected_index_list": [{"a": "bee", "pk_index": 4}],
        "unexpected_list": ["bee"]
      },
      "suppress_test_for": ["sqlite", "mssql"]
    },
    {
      "title": "negative_test_one_missing_value_and_insufficent_mostly",
      "exact_match_out" : false,
      "in": {
        "column": "b",
        "regex": "^a",
        "mostly": 0.8
      },
      "out": {
        "success":false,
        "unexpected_index_list": [{"b": "bdd", "pk_index": 3}],
        "unexpected_list": ["bdd"]
      },
      "suppress_test_for": ["sqlite", "mssql"]
    },
    {
      "title": "positive_test_one_missing_value_and_exact_mostly",
      "exact_match_out" : false,
      "in": {
        "column": "b",
        "regex": "^a",
        "mostly": 0.75
      },
      "out": {
        "success":true,
        "unexpected_index_list": [{"b": "bdd", "pk_index": 3}],
        "unexpected_list": ["bdd"]
      },
      "suppress_test_for": ["sqlite", "mssql"]
    },
    {
      "title": "positive_test_one_missing_value_and_sufficent_mostly",
      "exact_match_out" : false,
      "in": {
        "column": "b",
        "regex": "^a",
        "mostly": 0.7
      },
      "out": {
        "success":true,
        "unexpected_index_list": [{"b": "bdd", "pk_index": 3}],
        "unexpected_list": ["bdd"]
      },
      "suppress_test_for": ["sqlite", "mssql"]
    },
    {
      "title": "positive_test_all_missing_values",
      "exact_match_out" : false,
      "in": {
        "column": "c",
        "regex": "^a"
      },
      "out": {
        "success": true,
        "unexpected_index_list": [],
        "unexpected_list": []
      },
      "suppress_test_for": ["sqlite", "mssql"]
    },
    {
      "title": "positive_test_all_missing_values_mostly",
      "exact_match_out" : false,
      "in": {
        "column": "c",
        "regex": "^a",
        "mostly": 0.2
      },
      "out": {
        "success": true,
        "unexpected_index_list": [],
        "unexpected_list": []
      },
      "suppress_test_for": ["sqlite", "mssql"]
    },
    {
      "title": "positive_test_empty_regex",
      "exact_match_out" : false,
      "in": {
        "column": "b",
        "regex": ""
      },
      "out": {
        "success": true,
        "unexpected_index_list": [],
        "unexpected_list": []
      },
      "suppress_test_for": ["sqlite", "mysql", "mssql"]
    },
    {
      "title": "positive_test_more_complicated_regex",
      "exact_match_out" : false,
      "in": {
        "column": "b",
        "regex": "[ab]{1}[a-d]{2}"
      },
      "out": {
        "success": true,
        "unexpected_index_list": [],
        "unexpected_list": []
      },
      "suppress_test_for": ["sqlite", "mssql"]
    },
    {
      "title": "positive_test_match_characters_not_at_the_beginning_of_string",
      "exact_match_out" : false,
      "in": {
        "column": "a",
        "regex": "b",
        "mostly": 0.4
      },
      "out": {
        "success": true,
        "unexpected_index_list": [{"a": "aaa", "pk_index": 0}, {"a": "acc", "pk_index": 2}, {"a": "add", "pk_index": 3}],
        "unexpected_list": ["aaa", "acc", "add"]
      },
      "suppress_test_for": ["sqlite", "mssql"]
    }]
  }]
}
