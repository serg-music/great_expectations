        "column": "b",
        "like_pattern": "[a]%",
        "mostly": 0.8
      },
      "out": {
        "success":false,
        "unexpected_index_list": [{"pk_index": 3, "b": "bdd"}],
        "unexpected_list": ["bdd"]
