{
  "expectation_type" : "expect_column_values_to_match_like_pattern",
  "datasets" : [{
    "data" : {
      "a" : ["aaa", "abb", "acc", "add", "bee"],
      "b" : ["aaa", "abb", "acc", "bdd", null],
      "c" : [ null,  null,  null,  null, null],
      "column_name with space": ["aaa", "abb", "acc", "add", "bee"]
    },
    "schemas": {
      "spark": {
        "a": "StringType",
        "b": "StringType",
        "c": "StringType",
        "column_name with space": "StringType"
      },
      "mssql": {
        "a": "VARCHAR",
        "b": "NVARCHAR",
        "c": "NVARCHAR",
        "column_name with space": "VARCHAR"
      }
    },
    "tests": [
    {
      "title": "negative_test_insufficient_mostly_and_one_non_matching_value",
      "exact_match_out" : false,
      "in": {
        "column": "a",
        "like_pattern": "[a]%",
        "mostly": 0.9
      },
      "out": {
        "success":false,
        "unexpected_index_list": [{"pk_index": 4, "a": "bee"}],
        "unexpected_list": ["bee"]
      },
      "only_for": ["mssql"]
    },
    {
      "title": "positive_test_exact_mostly_w_one_non_matching_value",
      "exact_match_out" : false,
      "in": {
        "column": "a",
        "like_pattern": "[a]%",
        "mostly": 0.8
      },
      "out": {
        "success":true,
        "unexpected_index_list": [{"pk_index": 4, "a": "bee"}],
        "unexpected_list": ["bee"]
      },
      "only_for": ["mssql"]
    },
    {
      "title": "positive_test_column_name_has_space_mssql",
      "exact_match_out" : false,
      "in": {
        "column": "column_name with space",
        "like_pattern": "[a]%",
        "mostly": 0.8
      },
      "out": {
        "success":true,
        "unexpected_index_list": [{"pk_index": 4, "column_name with space": "bee"}],
        "unexpected_list": ["bee"]
      },
      "only_for": ["mssql"]
    },
    {
      "title": "positive_test_sufficient_mostly_w_one_non_matching_value",
      "exact_match_out" : false,
