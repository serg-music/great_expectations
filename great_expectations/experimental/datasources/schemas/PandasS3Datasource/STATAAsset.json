{
    "title": "STATAAsset",
    "description": "Read Stata file into DataFrame.",
    "type": "object",
    "properties": {
        "name": {
            "title": "Name",
            "type": "string"
        },
        "type": {
            "title": "Type",
            "default": "stata",
            "enum": [
                "stata"
            ],
            "type": "string"
        },
        "order_by": {
            "title": "Order By",
            "type": "array",
            "items": {
                "$ref": "#/definitions/BatchSorter"
            }
        },
        "batching_regex": {
            "title": "Batching Regex",
            "type": "string",
            "format": "regex"
        },
        "convert_dates": {
            "title": "Convert Dates",
            "default": true,
            "type": "boolean"
        },
        "convert_categoricals": {
            "title": "Convert Categoricals",
            "default": true,
            "type": "boolean"
        },
        "index_col": {
            "title": "Index Col",
            "type": "string"
        },
        "convert_missing": {
            "title": "Convert Missing",
            "default": false,
            "type": "boolean"
        },
        "preserve_dtypes": {
            "title": "Preserve Dtypes",
            "default": true,
            "type": "boolean"
        },
        "columns": {
            "title": "Columns",
            "type": "array",
            "items": {
                "type": "string"
            }
        },
        "order_categoricals": {
            "title": "Order Categoricals",
            "default": true,
            "type": "boolean"
        },
        "chunksize": {
            "title": "Chunksize",
            "type": "integer"
        },
        "iterator": {
            "title": "Iterator",
            "default": false,
            "type": "boolean"
        },
        "compression": {
            "title": "Compression",
            "default": "infer",
            "anyOf": [
                {
                    "enum": [
                        "infer",
                        "gzip",
                        "bz2",
                        "zip",
                        "xz",
                        "zstd",
                        "tar"
                    ],
                    "type": "string"
                },
                {
                    "type": "object"
                }
            ]
        },
        "storage_options": {
            "title": "Storage Options",
            "type": "object"
        }
    },
    "required": [
        "name",
        "batching_regex"
    ],
    "additionalProperties": false,
    "definitions": {
        "BatchSorter": {
            "title": "BatchSorter",
            "type": "object",
            "properties": {
                "key": {
                    "title": "Key",
                    "type": "string"
                },
                "reverse": {
                    "title": "Reverse",
                    "default": false,
                    "type": "boolean"
                }
            },
            "required": [
                "key"
            ]
        }
    }
}
